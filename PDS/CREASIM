//SIST19K JOB CLASS=C,MSGCLASS=C,NOTIFY=&SYSUID
//*-----------------------------------------------------------------
//*
//*   CHANGE "CSF.SCSFCKDS" BY YOUR CKDS
//*
//*-----------------------------------------------------------------
//DELETE   EXEC PGM=CSFKGUP,PARM=('SSM')
//CSFCKDS  DD DSN=CSF.SCSFCKDS,DISP=OLD
//CSFDIAG  DD SYSOUT=*,DCB=(LRECL=133,RECFM=FBA,DSORG=PS)
//CSFKEYS  DD SYSOUT=*,DCB=(LRECL=208,RECFM=FB,DSORG=PS)
//CSFSTMNT DD SYSOUT=*,DCB=(LRECL=80,RECFM=FB,DSORG=PS)
//CSFIN    DD *
DELETE  TYPE(DATA)    LAB(DESRANAS)
DELETE  TYPE(DATA)    LAB(ASMKEYCREATE)
DELETE  TYPE(DATA)    LAB(DESPRU)
DELETE  TYPE(DATA)    LAB(DESPRU2)
DELETE  TYPE(MAC)     LAB(MACPRU)
DELETE  TYPE(MACVER)  LAB(MACVERPRU)
//*-----------------------------------------------------------------
//CREATE   EXEC PGM=CSFKGUP,PARM=('SSM')
//CSFCKDS  DD DSN=CSF.SCSFCKDS,DISP=OLD
//CSFDIAG  DD SYSOUT=*,DCB=(LRECL=133,RECFM=FBA,DSORG=PS)
//CSFKEYS  DD SYSOUT=*,DCB=(LRECL=208,RECFM=FB,DSORG=PS)
//CSFSTMNT DD SYSOUT=*,DCB=(LRECL=80,RECFM=FB,DSORG=PS)
//CSFIN    DD *
ADD TYPE(DATA),
CLEAR KEY(9158596809158595,6201010101010101,0123456789ABCDEF) DES,
 LAB(DESPRU)
ADD TYPE(DATA) LENGTH(16),
CLEAR DES,
 LAB(DESPRU2)
ADD TYPE(MAC),
CLEAR KEY(0123456789ABCDEF),
 LAB(MACPRU)
ADD TYPE(MACVER),
CLEAR KEY(0123456789ABCDEF),
 LAB(MACVERPRU)
//*--------------------------------------------------------------------
//* REFRES DEL CKDS
//*--------------------------------------------------------------------
//REFRESH  EXEC PGM=CSFEUTIL,PARM='CSF.SCSFCKDS,REFRESH'
//SYSPRINT DD  SYSOUT=*
//*--------------------------------------------------------------------
